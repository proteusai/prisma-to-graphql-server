{
  "name": "prisma-to-graphql-server",
  "description": "Provide a description about your generator",
  "version": "1.0.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-to-graphql-server": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "5.18.0",
    "@prisma/generator-helper": "5.18.0",
    "@prisma/internals": "5.18.0",
    "@prisma/sdk": "^4.1.0-integration-fix-disconnect-dmmf.1",
    "pluralize": "^8.0.0",
    "prettier": "2.5.1",
    "semver": "^7.6.3",
    "ts-morph": "^25.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.21",
    "@types/pluralize": "^0.0.33",
    "@types/prettier": "2.4.2",
    "@types/semver": "^7.5.8",
    "jest": "27.4.7",
    "prisma": "6.2.0",
    "semantic-release": "^18.0.1",
    "ts-jest": "27.1.4",
    "typescript": "5.5.4"
  },
  "homepage": "Link to homepage or github readme here",
  "repository": {
    "type": "git",
    "url": "Repo link here"
  },
  "author": "Your Name <Your Email>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
